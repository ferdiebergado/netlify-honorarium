{
  "name": "netlify-honorarium",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "netlify dev -c vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --cache --cache-location .eslintcache",
    "preview": "vite preview",
    "format": "pnpm exec prettier . --write",
    "turso-dump": "turso db shell honorarium .dump > dump.sql",
    "turso-migrate": "turso db shell honorarium < init.sql",
    "create-db": "sqlite3 local.db '.read init.sql'",
    "db": "sqlite3 -init .sqliterc local.db",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@benedicte/docx-merge": "^0.1.3",
    "@fontsource-variable/inter": "^5.2.8",
    "@hookform/resolvers": "^5.2.2",
    "@libsql/client": "^0.15.15",
    "@netlify/functions": "^5.1.2",
    "@tabler/icons-react": "^3.36.1",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-query": "^5.90.16",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cookie": "^1.1.1",
    "docx": "^9.5.1",
    "exceljs": "^4.4.0",
    "googleapis": "^169.0.0",
    "next-themes": "^0.4.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-error-boundary": "^6.0.1",
    "react-hook-form": "^7.69.0",
    "react-router": "^7.11.0",
    "shadcn": "^3.6.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "to-words": "^4.9.0",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@types/node": "^24.10.4",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "4.0.16",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-dom": "^2.5.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^16.5.0",
    "prettier": "3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.51.0",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  }
}
